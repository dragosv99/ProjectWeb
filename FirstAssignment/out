		<div class="input">
			<p class="error"></p>
			<input type="text" placeholder="Uw E-mail" value="" name="" id="email" size="20" class="">
			<input type="submit" value="Aanmelden" class="button secondary right eTrack_emailSub" name="">
			<p class="success"></p>
		</div>
	</div>
</div> 


<script type="text/javascript">
	
	$(".emailSub.sidebar input[type='submit']").click(function(e) {
		e.preventDefault();
		var email = $('.emailSub.sidebar input#email').val();

		if (email.indexOf("@") === -1) {
			$(".emailSub.sidebar .error").html("Invalid Email!");
		} else {
			do_ajax(email);
		}
	});

	function do_ajax(email) {
	    var email = email;
	    
	    $.ajax({
	        url: "/fileadmin/mail/form.php",
	        type: "POST",
	        data: { email: email, locale: 'nl_nl' },
	        context: $(".emailSub.sidebar"),
	        success: function(data){
	            $('.input .success', this).html(data);
	        }
	    });
    }

</script>

