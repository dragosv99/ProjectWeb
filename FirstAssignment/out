<script type="text/javascript">
    $(document).ready(function () {

        var clickMapConfig = {
            id: 'tempHome-6-31',
            dataURL_static: '/fileadmin/maps/tempHome/6/31/static.json',
            dataURL_dyn:    '/index.php?id=110'+
                            '&day=Tuesday'+
                            '&now='+
                            '&data_format=json&context=tempHome%2F6%2F31'+
                            '&map_id=59',
            disableRegions: '0',
            // zoomEnabled: true & zoomMax: 1 is needed to prevent zooming and panning on mobile devices (see mobile frontpage clickmap config)
            zoomEnabled: '',
            zoomMax: '',
            forecastTargetPageId: '18',
            locale: 'nl_nl'
        };

        var forecastClickMapComponent =  new ForecastClickMap(clickMapConfig);

        $layerButtons = $('button[data-layer]');
        $daytimeButton = $('button.daytime');

        /* Layer Switch */
        $layerButtons.click(function() {
            forecastClickMapComponent.switchForecastMapLayer(this.dataset.layer, $layerButtons);
        });

        /* Day/Night Switch */
        $daytimeButton.click(function() {
            forecastClickMapComponent.switchForecastMapDaytime(this);
        });

        forecastClickMapComponent.loadForecastMapLayers($layerButtons, $daytimeButton);

    });

</script>
    



